{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"],
      "@hooks/*": ["./hooks/*"],
      "@components/*": ["./components/*"],
      "@assets/*": ["./assets/*"],
      "@constants/*": ["./constants/*"],
      "@data/*": ["./data/*"],
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "**/*.js", "**/*.jsx"]
}
